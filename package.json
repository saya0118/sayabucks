{
  "name": "vite-react-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "style:all": "concurrently 'npm:style:lint' 'npm:style:css' 'npm:style:prettier' 'npm:check-types'",
    "style:css": "npx stylelint 'src/**/*.css'",
    "style:css:fix": "npm run style:css -- --fix",
    "style:lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "style:lint:fix": "npm run style:lint -- --fix",
    "style:prettier": "npx prettier --check '**/*.{js,jsx,ts,tsx}'",
    "style:prettier:fix": "npm run style:prettier -- --write",
    "fix": "npm run style:prettier:fix && concurrently 'npm:style:lint:fix' 'npm:style:css:fix'",
    "check-types": "tsc",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.17",
    "@types/node": "^17.0.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-react": "^1.1.4",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.2",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.13"
  }
}
