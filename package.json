{
  "name": "vite-react-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "style:all": "concurrently 'npm:style:lint' 'npm:style:css' 'npm:style:prettier' 'npm:check-types'",
    "style:css": "npx stylelint 'src/**/*.css'",
    "style:css:fix": "npm run style:css -- --fix",
    "style:lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "style:lint:fix": "npm run style:lint -- --fix",
    "style:prettier": "npx prettier --check '**/*.{js,jsx,ts,tsx}'",
    "style:prettier:fix": "npm run style:prettier -- --write",
    "fix": "npm run style:prettier:fix && concurrently 'npm:style:lint:fix' 'npm:style:css:fix'",
    "check-types": "tsc",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/babel__core": "^7.1.19",
    "@types/node": "^18.14.2",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-react": "^1.3.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "cypress": "^9.5.3",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.2",
    "prettier": "^2.5.1",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.13",
    "vite-plugin-react-svg": "^0.2.0"
  }
}
